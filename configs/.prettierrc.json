{
    "printWidth": 80,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "consistent",
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "bracketSameLine": false,
    "arrowParens": "always",
    "overrides": [
        {
            "files": "prettierrc",
            "options": {"trailingComma": "none", "insertPragma": false}
        }
    ]
}
